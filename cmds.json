{
  "name":"I am just a name",
  "cmds" : [
    {
      "name":"show scala phases",
      "cmd":"scalac -Xshow-phases"
    },
    {
      "name":"warn dead code if.scala",
      "cmd":"scalac -Ywarn-dead-code if.scala"
    },
    {
      "name":"print all phases",
      "cmd":"scalac -Xprint:all if.scala"
    },
    {
      "name":"abstract method",
      "cmd":"scalac -Xprint:all param.scala"
    },
    {
      "name":"refcheck",
      "cmd":"scalac -Xprint:all refchecks.scala"
    }
  ]
}